<h1>Workorders</h1>
<table>
	<thead>
		<tr>
			<th>ID</th>
			<th>Date Created</th>
			<th>Date Completed</th>
			<th>Assigned To</th>
			<th>Created By</th>
			<th>Priority</th>
			<th>Status</th>
			<th>State</th>
		</tr>
	</thead>
	<tbody>
		{{#each model as |wo|}}
		<tr {{action "goToDetails" wo on="doubleClick"}}>
			<td>{{wo.id}}</td>
			<td>{{shortDate wo.dateCreated}}</td>
			<td>{{shortDate wo.dateCompleted}}</td>
			<td>{{wo.assignedTo}}</td>
			<td>{{wo.createdBy}}</td>
			<td>{{wo.priority}}</td>
			<td>{{wo.status}}</td>
			<td>{{wo.state}}</td>
		</tr>
		{{/each}}
	</tbody>
</table>
